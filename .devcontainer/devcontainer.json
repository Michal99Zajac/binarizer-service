{
  "name": "Binarizer Service",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "forwardPorts": [
    8000
  ],
  "containerUser": "root",
  "remoteUser": "root",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.black-formatter",
        "ms-azuretools.vscode-docker",
        "EditorConfig.EditorConfig",
        "ms-python.flake8",
        "ms-python.isort",
        "magicstack.MagicPython",
        "ms-python.vscode-pylance",
        "ms-python.python",
        "rust-lang.rust-analyzer"
      ]
    }
  }
}
